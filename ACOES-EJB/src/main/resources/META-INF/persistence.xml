<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence">
    <persistence-unit name="ACOES" transaction-type="JTA">
        <class>com.SII.entidades.Becado</class>
        <class>com.SII.entidades.Beneficiario</class>
        <class>com.SII.entidades.Proyecto</class>
        <class>com.SII.entidades.Socio</class>
        <class>com.SII.entidades.Usuario</class>
        <exclude-unlisted-classes>false</exclude-unlisted-classes>
        <properties>
            <property name="javax.persistence.schema-generation.database.action" value="create"/>
        </properties>
    </persistence-unit>
</persistence>
